# Example Environment Configuration
# Copy this file to .env.development for local development
# The .env.production file is used in production and populated by CI

# Server Configuration
PORT=3001
NODE_ENV=development

# OpenAI Configuration
OPENAI_API_KEY=your_api_key_here

# Database Configuration
DATABASE_URL="postgresql://user:password@localhost:5432/dbname?schema=public"
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_db_password
POSTGRES_DB=your_db_name
POSTGRES_PORT=5432
POSTGRES_HOST=localhost

# TheNewsAPI Configuration
NEWS_API_TOKEN=your_news_api_token_here
NEWS_API_BASE_URL=https://api.thenewsapi.com/v1/news

# Financial Datasets API Configuration
FINANCIAL_DATASETS_API_KEY=your_financial_datasets_api_key_here
FINANCIAL_DATASETS_API_BASE_URL=https://api.financialdatasets.ai
